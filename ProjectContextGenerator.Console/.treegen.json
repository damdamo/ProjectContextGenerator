{
  "version": 1,
  "root": ".",
  "maxDepth": 3,
  "exclude": [
    "bin/",
    "obj/",
    ".git/",
    "node_modules/",
    ".github"
  ],
  "gitIgnore": "Nested",
  "directoriesOnly": false,
  "history": {
    "last": 20,
    "maxBodyLines": 6,
    "detail": "Titles",
    "includeMerges": false
  },
  "profiles": {
    "fast": {
      "maxDepth": 1,
      "directoriesOnly": true
    },
    "full": {
      "maxDepth": -1,
      "collapseSingleChildDirectories": false,
      "history": {
        "detail": "TitleAndBody"
      }
    },
    "csharp": {
      "include": [ "**/*.cs", "**/*.csproj" ],
      "exclude": [ "bin/", "obj/" ]
    }
  }
}